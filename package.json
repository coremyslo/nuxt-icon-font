{
  "name": "@coremyslo/nuxt-icon-font",
  "version": "0.0.3",
  "description": "Nuxt icon font generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/coremyslo/nuxt-icon-font"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@coremyslo/asset-generator": "^1.0.0",
    "@coremyslo/font-generator": "^1.1.3",
    "@coremyslo/icon-generator": "^2.1.0",
    "@nuxt/kit": "^3.3.3",
    "browserslist": "^4.21.5",
    "case": "^1.6.3",
    "fontslist": "^1.0.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.3.3",
    "@nuxt/test-utils": "^3.3.3",
    "changelogen": "^0.5.2",
    "eslint": "^8.37.0",
    "nuxt": "^3.3.3",
    "vitest": "^0.29.8"
  }
}
